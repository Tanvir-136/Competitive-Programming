#include<iostream>
#include<bits/stdc++.h>
#define FAST ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0)
#define ll long long int
using namespace std;
void solve(){
    int n;
    cin >> n;
    vector<int> v(n);
    for (int i = 0; i < n; ++i) {
        cin >> v[i];
    }
    int min_Time = *min_element(v.begin(), v.end());
    int cnt = count(v.begin(), v.end(), min_Time);

    if (cnt == 1) {
        auto result = find(v.begin(), v.end(), min_Time);
        cout << result - v.begin() + 1 << endl;
    } 
    else {
        cout << "Still Rozdil" << endl;
    }
}
int main(){
    FAST;
    //Start Here
    int t=1;
    //cin >> t;
    while (t--){
        solve();
    }
    return 0;
}
